{
    "Logging": {
        "LogLevel": {
            "Default": "Information"
        },
        "Console": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": true
        },
        "EventLog": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": true
        },
        "EventSource": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": true
        },
        "DevInsights": {
            "LogLevel": {
                "Default": "Information",
                "Microsoft.AspNetCore.*": "Warning"
            },
            "IncludeScopes": true
        },
        "Debug": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": true
        },
        //#if (appInsights == true)
        "ApplicationInsights": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": false
        },
        "ApplicationInsightsLoggerProvider": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": false
        },
        //#endif
        //#if (appCenter == true)
        "AppCenterLoggerProvider": {
            "LogLevel": {
                "Default": "Information"
            },
            "IncludeScopes": false
        }
        //#endif
    },
    "$schema": "https://json.schemastore.org/appsettings.json"
}