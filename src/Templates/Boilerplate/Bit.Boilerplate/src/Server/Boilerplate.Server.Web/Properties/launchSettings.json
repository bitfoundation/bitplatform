{
    "profiles": {
        "Boilerplate.Server.Web": {
            "commandName": "Project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "IN_APP_SCSS_COMPILER_ENABLED": "true"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://*:5030"
        },
        //#if (api == "Integrated")
        "Boilerplate.Server.Web-Scalar": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "scalar",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://*:5030"
        },
        //#endif
        "Boilerplate.Server.Web-BlazorWebAssembly": {
            "commandName": "Project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "WebAppRender:BlazorMode": "BlazorWebAssembly",
                "IN_APP_SCSS_COMPILER_ENABLED": "true"
            },
            "dotnetRunMessages": true,
            "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
            "applicationUrl": "http://*:5030"
        },
        "WSL": {
            "commandName": "WSL2",
            "launchBrowser": true,
            "launchUrl": "http://localhost:5030/",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://*:5030"
            }
        },
        "Docker": {
            "commandName": "SdkContainer",
            "launchBrowser": true,
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/",
            "environmentVariables": {
                "ASPNETCORE_HTTP_PORTS": "5030",
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ConnectionStrings__sqlite": "Data Source=/container_volume/App_Data/BoilerplateDb.db;"
            },
            "DockerfileRunArguments": "-v C:\\DockerVolumes\\AC87AA5B-4B37-4E52-8468-2D5DF24AF256:/container_volume",
            "publishAllPorts": true,
            "useSSL": false,
            "httpPort": 5030
        },
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "IN_APP_SCSS_COMPILER_ENABLED": "true"
            }
        },
        "IIS": {
            "commandName": "IIS",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "IN_APP_SCSS_COMPILER_ENABLED": "true"
            }
        }
    },
    "iisSettings": {
        "windowsAuthentication": false,
        "anonymousAuthentication": true,
        "iis": {
            "applicationUrl": "http://localhost:5030/"
        },
        "iisExpress": {
            "applicationUrl": "http://localhost:5030/"
        }
    },
    "$schema": "http://json.schemastore.org/launchsettings.json"
}