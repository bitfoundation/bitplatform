<PageTitle>bit BlazorUI</PageTitle>
<LayoutView Layout="@typeof(MainLayout)">
    <Router AppAssembly="@GetType().Assembly"
            AdditionalAssemblies="@(AssemblyLoadContext.Default.Assemblies.Where(asm => asm.GetName().Name?.Contains("Bit.BlazorUI.Demo") is true))">
        <Found Context="routeData">
            <RouteView RouteData="@routeData" />
        </Found>
        <NotFound>
            <NotFoundComponent />
        </NotFound>
        <Navigating>
            <LoadingComponent />
        </Navigating>
    </Router>
</LayoutView>