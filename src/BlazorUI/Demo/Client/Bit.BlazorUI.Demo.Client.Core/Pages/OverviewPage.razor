@page "/overview"
@page "/components"

@code {
    private IEnumerable<IBitComponentParams> _cascadingParams = new List<IBitComponentParams>
    {
        new BitCardParams { Background = BitColorKind.Primary, FullWidth = true, Style = "padding: 1.5rem" },
        new BitTextParams { Typography = BitTypography.Body1 }
    };
}

<PageOutlet Url="overview"
            Title="Overview"
            Description="discover bit BlazorUI at a glance, explore component categories, and jump-start a project." />


<section class="page-container">
    <BitParams Parameters="_cascadingParams">
        <div class="overview-hero">
            <BitText Typography="BitTypography.H1" Gutter>bit BlazorUI</BitText>
            <BitText Typography="BitTypography.H5" Gutter Style="color: var(--bit-color-foreground-secondary); max-width: 800px;">
                Production-ready and Fluent-inspired components for Blazor that stay native, flexible,
                and fast across all interactive Blazor modes (WASM, Server, Hybrid, and pre-rendering).
            </BitText>
            <BitStack Horizontal Gap="1rem" Style="margin-top: 1.5rem;">
                <BitButton Href="/getting-started" Size="BitSize.Large" IconName="@BitIconName.ChevronRightSmall" ReversedIcon>
                    Get Started
                </BitButton>
                <BitButton Href="https://github.com/bitfoundation/bitplatform" Target="_blank" Variant="BitVariant.Outline" Size="BitSize.Large">
                    <BitIcon IconName="@BitIconName.Code" Style="margin-right: 0.5rem;" />
                    View on GitHub
                </BitButton>
            </BitStack>
        </div>

        <br /><br />

        <!-- Quick Stats -->
        <div class="quick-stats-container">
            <BitStack Horizontal Wrap Alignment="BitAlignment.SpaceBetween" Gap="1.5rem">
                <BitCard Background="BitColorKind.Secondary" Class="stat-card">
                    <BitStack Alignment="BitAlignment.Center" Gap="1rem">
                        <div class="stat-icon-wrapper">
                            <BitIcon Style="font-size:2.5rem"
                                     IconName="@BitIconName.AppIconDefault"
                                     Color="BitColor.PrimaryForeground" />
                        </div>
                        <BitText Style="font-weight:bold"
                                 Typography="BitTypography.H2">
                            80+
                        </BitText>
                        <BitText Color="BitColor.SecondaryForeground">
                            Components
                        </BitText>
                    </BitStack>
                </BitCard>

                <BitCard Background="BitColorKind.Secondary" Class="stat-card">
                    <BitStack Alignment="BitAlignment.Center" Gap="1rem">
                        <div class="stat-icon-wrapper">
                            <BitIcon Style="font-size:2.5rem"
                                     IconName="@BitIconName.Download"
                                     Color="BitColor.PrimaryForeground" />
                        </div>
                        <BitText Style="font-weight:bold"
                                 Typography="BitTypography.H2">
                            ~400KB
                        </BitText>
                        <BitText Color="BitColor.SecondaryForeground">
                            Total suit bundle size
                        </BitText>
                    </BitStack>
                </BitCard>

                <BitCard Background="BitColorKind.Secondary" Class="stat-card">
                    <BitStack Alignment="BitAlignment.Center" Gap="1rem">
                        <div class="stat-icon-wrapper">
                            <BitIcon IconName="@BitIconName.Code" Style="font-size: 2.5rem; color: var(--bit-color-primary-main);" />
                        </div>
                        <BitText Typography="BitTypography.H2" Style="color: var(--bit-color-primary-main); font-weight: 600;">
                            .NET 8+
                        </BitText>
                        <BitText Typography="BitTypography.Body1" Style="color: var(--bit-color-foreground-secondary)">
                            Framework
                        </BitText>
                    </BitStack>
                </BitCard>
            </BitStack>
        </div>

        <br />

        <BitCard>
            <BitText Typography="BitTypography.H4" Gutter>Why choose bit BlazorUI?</BitText>
            <br />
            <BitStack Gap="1.5rem">
                <BitStack Horizontal Gap="1rem" Alignment="BitAlignment.Start">
                    <BitIcon IconName="@BitIconName.CheckMark" Style="color: var(--bit-color-success-main); font-size: 1.5rem; flex-shrink: 0;" />
                    <BitStack Gap="0.25rem">
                        <BitText Typography="BitTypography.H6">Native Blazor Performance</BitText>
                        <BitText Typography="BitTypography.Body2" Style="color: var(--bit-color-foreground-secondary);">
                            Built entirely in C# with optimized rendering for all Blazor hosting models. Using 5 components adds only ~50KB, and 40+ components adds just ~200KB total.
                        </BitText>
                    </BitStack>
                </BitStack>
                @* <BitStack Horizontal Gap="1rem" Alignment="BitAlignment.Start">
                    <BitIcon IconName="@BitIconName.CheckMark" Style="color: var(--bit-color-success-main); font-size: 1.5rem; flex-shrink: 0;" />
                    <BitStack Gap="0.25rem">
                        <BitText Typography="BitTypography.H6">Accessibility First</BitText>
                        <BitText Typography="BitTypography.Body2" Style="color: var(--bit-color-foreground-secondary);">
                            Built-in theme and RTL support with WCAG-compliant components that work seamlessly with screen readers and keyboard navigation.
                        </BitText>
                    </BitStack>
                </BitStack> *@
                <BitStack Horizontal Gap="1rem" Alignment="BitAlignment.Start">
                    <BitIcon IconName="@BitIconName.CheckMark" Style="color: var(--bit-color-success-main); font-size: 1.5rem; flex-shrink: 0;" />
                    <BitStack Gap="0.25rem">
                        <BitText Typography="BitTypography.H6">Comprehensive Documentation</BitText>
                        <BitText Typography="BitTypography.Body2" Style="color: var(--bit-color-foreground-secondary);">
                            Extensive demos, API reference, usage guidance, and source code available for every component. Get started quickly with clear examples.
                        </BitText>
                    </BitStack>
                </BitStack>
                <BitStack Horizontal Gap="1rem" Alignment="BitAlignment.Start">
                    <BitIcon IconName="@BitIconName.CheckMark" Style="color: var(--bit-color-success-main); font-size: 1.5rem; flex-shrink: 0;" />
                    <BitStack Gap="0.25rem">
                        <BitText Typography="BitTypography.H6">Production Ready</BitText>
                        <BitText Typography="BitTypography.Body2" Style="color: var(--bit-color-foreground-secondary);">
                            Optimized for .NET 8.0+ with SSR-friendly styles, responsive layouts, and light/dark themes ready for customization.
                        </BitText>
                    </BitStack>
                </BitStack>
            </BitStack>
        </BitCard>

        <br />

