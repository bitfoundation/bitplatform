@page "/not-found"

@inherits AppComponentBase

<BitStack Alignment="BitAlignment.Center">
    <BitText Typography="BitTypography.H1">404</BitText>
    <BitText>There is nothing here: [<b>@Url</b>]</BitText>
</BitStack>

@code {
    [SupplyParameterFromQuery(Name = "url")] public string? Url { get; set; }
}